{
  "name": "union-beta",
  "author": "@unioncredit",
  "description": "Union Finance Beta Application",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PUBLIC_VERCEL_GIT_COMMIT_SHA=$(git rev-parse HEAD) next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "format": "prettier --ignore-path .gitignore --write \"**/*.js\" \"**/*.tsx\" \"**/*.ts\"",
    "lint": "eslint . --ext js --cache",
    "vercel-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache",
      "pretty-quick --staged"
    ]
  },
  "dependencies": {
    "@ensdomains/ensjs": "^2.0.1",
    "@ethersproject/address": "^5.0.5",
    "@ethersproject/bignumber": "^5.0.8",
    "@ethersproject/contracts": "^5.0.5",
    "@ethersproject/providers": "^5.0.14",
    "@ethersproject/units": "^5.0.6",
    "@web3-react/core": "^6.1.1",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.1.5",
    "@web3-react/walletconnect-connector": "^6.2.0",
    "add-event-to-calendar": "^1.0.7",
    "canvas": "2.6.1",
    "canvas-confetti": "^1.3.2",
    "cookie-store": "^3.0.0",
    "dayjs": "^1.9.6",
    "eth-permit": "^0.1.8",
    "ethereum-blockies-base64": "^1.0.2",
    "fathom-client": "^3.0.0",
    "graphql": "^15.6.0",
    "graphql-request": "^3.5.0",
    "md5": "^2.3.0",
    "next": "^10.0.1",
    "next-svgr": "^0.0.2",
    "parse-ms": "^2.1.0",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.11.3",
    "react-markdown": "^5.0.2",
    "react-ridge-state": "4.1.6",
    "react-use": "^15.3.4",
    "swr": "^0.3.9",
    "union-ui": "0.2.31"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^10.0.1",
    "@types/canvas-confetti": "^1.3.0",
    "@types/react": "17.0.1",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "hooks.macro": "^1.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "source-map": "^0.7.3",
    "typescript": "^4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unioncredit/union-beta.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/unioncredit/union-beta/issues"
  },
  "homepage": "https://github.com/unioncredit/union-beta#readme"
}
